{
  "name": "kata-cli",
  "version": "0.1.0",
  "description": "Command line tools for Diaenne",
  "main": "index.js",
  "bin": {
    "kata": "./bin/kata.js"
  },
  "directories": {
    "test": "test"
  },
  "scripts": {
    "postinstall": "tsc",
    "clean": "rm -rf ./lib",
    "build": "tsc",
    "test": "NODE_ENV=test mocha --recursive --compilers ts:ts-node/register --bail",
    "cover": "sh bin/coverage.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org:yesboss/kata-cli.git"
  },
  "keywords": [
    "cli"
  ],
  "nyc": {
    "include": [
      "components/**/*.ts",
      "components/*.ts",
      "interfaces/*.ts"
    ],
    "exclude": [
      "node_modules"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "json",
      "html"
    ],
    "all": true
  },
  "author": "Febry Antonius",
  "license": "ISC",
  "homepage": "https://bitbucket.org/yesboss/kata-cli#readme",
  "dependencies": {
    "@types/node": "^7.0.28",
    "@types/node-uuid": "0.0.28",
    "commander": "^2.9.0",
    "deasync": "^0.1.10",
    "inquirer": "^3.0.6",
    "js-yaml": "^3.8.2",
    "merapi": "^0.17.1",
    "merapi-proxy": "^0.1.3",
    "node-uuid": "^1.4.8",
    "superagent": "^3.5.2"
  },
  "devDependencies": {
    "@types/node": "^7.0.28",
    "@types/node-uuid": "0.0.28",
    "codacy-coverage": "^2.0.2",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "^11.1.0",
    "ts-node": "^3.3.0",
    "typescript": "^2.4.2"
  }
}
